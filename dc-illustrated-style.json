{
  "version": 8,
  "name": "DC Illustrated Map",
  "metadata": {
    "mapbox:autocomposite": true,
    "mapbox:type": "template",
    "mapbox:sdk-support": {
      "js": "2.0.0",
      "android": "9.0.0",
      "ios": "5.9.0"
    }
  },
  "center": [-77.0369, 38.9072],
  "zoom": 11,
  "bearing": 0,
  "pitch": 0,
  "sources": {
    "composite": {
      "url": "mapbox://mapbox.mapbox-streets-v8,mapbox.mapbox-terrain-v2",
      "type": "vector"
    }
  },
  "sprite": "mapbox://sprites/mapbox/outdoors-v12",
  "glyphs": "mapbox://fonts/mapbox/{fontstack}/{range}.pbf",
  "layers": [
    {
      "id": "background",
      "type": "background",
      "paint": {
        "background-color": "#EFE6D5"
      }
    },
    {
      "id": "landcover-grass",
      "type": "fill",
      "source": "composite",
      "source-layer": "landcover",
      "filter": ["==", ["get", "class"], "grass"],
      "paint": {
        "fill-color": "#8BC34A",
        "fill-opacity": 0.9
      }
    },
    {
      "id": "landcover-wood",
      "type": "fill",
      "source": "composite",
      "source-layer": "landcover",
      "filter": ["==", ["get", "class"], "wood"],
      "paint": {
        "fill-color": "#5C8F2F",
        "fill-opacity": 0.85
      }
    },
    {
      "id": "landuse-park",
      "type": "fill",
      "source": "composite",
      "source-layer": "landuse",
      "filter": ["==", ["get", "class"], "park"],
      "paint": {
        "fill-color": "#7ED957",
        "fill-opacity": 1
      }
    },
    {
      "id": "landuse-park-outline",
      "type": "line",
      "source": "composite",
      "source-layer": "landuse",
      "filter": ["==", ["get", "class"], "park"],
      "paint": {
        "line-color": "#4A7C24",
        "line-width": 1.5
      }
    },
    {
      "id": "national-park",
      "type": "fill",
      "source": "composite",
      "source-layer": "landuse_overlay",
      "filter": ["==", ["get", "class"], "national_park"],
      "paint": {
        "fill-color": "#7ED957",
        "fill-opacity": 0.9
      }
    },
    {
      "id": "water",
      "type": "fill",
      "source": "composite",
      "source-layer": "water",
      "paint": {
        "fill-color": "#5DA5DB"
      }
    },
    {
      "id": "water-outline",
      "type": "line",
      "source": "composite",
      "source-layer": "water",
      "paint": {
        "line-color": "#3A7CA5",
        "line-width": 2
      }
    },
    {
      "id": "waterway",
      "type": "line",
      "source": "composite",
      "source-layer": "waterway",
      "paint": {
        "line-color": "#5DA5DB",
        "line-width": [
          "interpolate",
          ["linear"],
          ["zoom"],
          10, 1,
          14, 3,
          18, 6
        ]
      }
    },
    {
      "id": "building",
      "type": "fill",
      "source": "composite",
      "source-layer": "building",
      "minzoom": 13,
      "paint": {
        "fill-color": "#C1604A",
        "fill-opacity": [
          "interpolate",
          ["linear"],
          ["zoom"],
          13, 0,
          14, 1
        ]
      }
    },
    {
      "id": "building-extrusion",
      "type": "fill-extrusion",
      "source": "composite",
      "source-layer": "building",
      "minzoom": 14,
      "paint": {
        "fill-extrusion-color": "#C1604A",
        "fill-extrusion-height": [
          "interpolate",
          ["linear"],
          ["zoom"],
          14, 0,
          15, ["get", "height"]
        ],
        "fill-extrusion-base": 0,
        "fill-extrusion-opacity": 0.9
      }
    },
    {
      "id": "road-minor",
      "type": "line",
      "source": "composite",
      "source-layer": "road",
      "filter": [
        "all",
        ["==", ["get", "class"], "street"],
        ["!=", ["get", "type"], "construction"]
      ],
      "paint": {
        "line-color": "#E0D0B8",
        "line-width": [
          "interpolate",
          ["exponential", 1.5],
          ["zoom"],
          12, 0.5,
          14, 2,
          18, 8
        ]
      }
    },
    {
      "id": "road-minor-case",
      "type": "line",
      "source": "composite",
      "source-layer": "road",
      "filter": [
        "all",
        ["==", ["get", "class"], "street"],
        ["!=", ["get", "type"], "construction"]
      ],
      "paint": {
        "line-color": "#A0725F",
        "line-width": [
          "interpolate",
          ["exponential", 1.5],
          ["zoom"],
          12, 0.75,
          14, 3,
          18, 10
        ],
        "line-gap-width": [
          "interpolate",
          ["exponential", 1.5],
          ["zoom"],
          12, 0.5,
          14, 2,
          18, 8
        ]
      },
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      }
    },
    {
      "id": "road-street",
      "type": "line",
      "source": "composite",
      "source-layer": "road",
      "filter": [
        "all",
        ["==", ["get", "class"], "street"],
        ["!=", ["get", "type"], "construction"]
      ],
      "paint": {
        "line-color": "#E0D0B8",
        "line-width": [
          "interpolate",
          ["exponential", 1.5],
          ["zoom"],
          11, 0,
          12.5, 0.5,
          14, 2,
          18, 12
        ]
      },
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      }
    },
    {
      "id": "road-primary",
      "type": "line",
      "source": "composite",
      "source-layer": "road",
      "filter": [
        "all",
        ["==", ["get", "class"], "primary"],
        ["!=", ["get", "type"], "construction"]
      ],
      "paint": {
        "line-color": "#F2A65A",
        "line-width": [
          "interpolate",
          ["exponential", 1.5],
          ["zoom"],
          10, 1,
          14, 4,
          18, 18
        ]
      },
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      }
    },
    {
      "id": "road-primary-case",
      "type": "line",
      "source": "composite",
      "source-layer": "road",
      "filter": [
        "all",
        ["==", ["get", "class"], "primary"],
        ["!=", ["get", "type"], "construction"]
      ],
      "paint": {
        "line-color": "#A0725F",
        "line-width": [
          "interpolate",
          ["exponential", 1.5],
          ["zoom"],
          10, 1.5,
          14, 6,
          18, 22
        ],
        "line-gap-width": [
          "interpolate",
          ["exponential", 1.5],
          ["zoom"],
          10, 1,
          14, 4,
          18, 18
        ]
      },
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      }
    },
    {
      "id": "road-motorway",
      "type": "line",
      "source": "composite",
      "source-layer": "road",
      "filter": [
        "all",
        ["==", ["get", "class"], "motorway"],
        ["!=", ["get", "type"], "construction"]
      ],
      "paint": {
        "line-color": "#F2A65A",
        "line-width": [
          "interpolate",
          ["exponential", 1.5],
          ["zoom"],
          8, 1,
          14, 6,
          18, 24
        ]
      },
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      }
    },
    {
      "id": "road-label",
      "type": "symbol",
      "source": "composite",
      "source-layer": "road",
      "minzoom": 12,
      "filter": [
        "all",
        ["has", "name"],
        [
          "in",
          ["get", "class"],
          ["literal", ["street", "primary", "secondary", "tertiary"]]
        ]
      ],
      "layout": {
        "text-field": ["get", "name"],
        "text-font": ["DIN Pro Bold", "Arial Unicode MS Bold"],
        "text-size": [
          "interpolate",
          ["linear"],
          ["zoom"],
          12, 11,
          18, 16
        ],
        "symbol-placement": "line",
        "text-rotation-alignment": "map",
        "text-pitch-alignment": "viewport"
      },
      "paint": {
        "text-color": "#3E2723",
        "text-halo-color": "#FFFFFF",
        "text-halo-width": 2
      }
    },
    {
      "id": "place-label-city",
      "type": "symbol",
      "source": "composite",
      "source-layer": "place_label",
      "filter": ["==", ["get", "type"], "city"],
      "layout": {
        "text-field": ["get", "name"],
        "text-font": ["DIN Pro Bold", "Arial Unicode MS Bold"],
        "text-size": [
          "interpolate",
          ["linear"],
          ["zoom"],
          8, 14,
          12, 20,
          16, 28
        ]
      },
      "paint": {
        "text-color": "#3E2723",
        "text-halo-color": "#FFFFFF",
        "text-halo-width": 2.5
      }
    },
    {
      "id": "place-label-neighborhood",
      "type": "symbol",
      "source": "composite",
      "source-layer": "place_label",
      "filter": ["==", ["get", "type"], "neighborhood"],
      "minzoom": 12,
      "layout": {
        "text-field": ["get", "name"],
        "text-font": ["DIN Pro Medium", "Arial Unicode MS Regular"],
        "text-size": [
          "interpolate",
          ["linear"],
          ["zoom"],
          12, 11,
          16, 16
        ]
      },
      "paint": {
        "text-color": "#5D4037",
        "text-halo-color": "#FFFFFF",
        "text-halo-width": 2
      }
    },
    {
      "id": "poi-label",
      "type": "symbol",
      "source": "composite",
      "source-layer": "poi_label",
      "minzoom": 14,
      "layout": {
        "text-field": ["get", "name"],
        "text-font": ["DIN Pro Medium", "Arial Unicode MS Regular"],
        "text-size": 11,
        "icon-image": ["get", "maki"],
        "icon-size": 1,
        "text-anchor": "top",
        "text-offset": [0, 0.8]
      },
      "paint": {
        "text-color": "#3E2723",
        "text-halo-color": "#FFFFFF",
        "text-halo-width": 1.5
      }
    }
  ]
}

